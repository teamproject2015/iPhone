<Keyboard
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="@dimen/qwerty_default_key_horizontal_gap"
    android:keyHeight="@dimen/qwerty_default_key_height"
    android:keyWidth="@dimen/qwerty_default_key_width"
    android:verticalGap="@dimen/qwerty_default_key_vertical_gap">

    <Row >
        <Key android:horizontalGap="@dimen/qwerty_first_col_horizontal_gap" android:codes="49" android:keyEdgeFlags="left" android:keyLabel="1" />
        <Key android:codes="50" android:keyLabel="2" />
        <Key android:codes="51" android:keyLabel="3" />
        <Key android:codes="52" android:keyLabel="4" />
        <Key android:codes="53" android:keyLabel="5" />
        <Key android:codes="54" android:keyLabel="6" />
        <Key android:codes="55" android:keyLabel="7" />
        <Key android:codes="56" android:keyLabel="8" />
        <Key android:codes="57" android:keyLabel="9" />
        <Key android:codes="48" android:keyEdgeFlags="right" android:keyLabel="0" />
    </Row>
    <Row >
        <Key android:horizontalGap="@dimen/qwerty_first_col_horizontal_gap" android:codes="33" android:keyEdgeFlags="left" android:keyLabel="!" />
        <Key android:codes="64" android:keyLabel="\@" />
        <Key android:codes="35" android:keyLabel="\#" />
        <Key android:codes="36" android:keyLabel="$" />
        <Key android:codes="56" android:keyLabel="/" />
        <Key android:codes="45" android:keyLabel="-" />
        <Key android:codes="38" android:keyLabel="&amp;" />
        <Key android:codes="42" android:keyLabel="*" />
        <Key android:codes="40" android:keyLabel="(" />
        <Key android:codes="41" android:keyEdgeFlags="right" android:keyLabel=")" />
    </Row>
    <Row >
        <Key android:codes="-1"  android:keyWidth="@dimen/qwerty_l_shift_key_width" android:horizontalGap="@dimen/qwerty_first_col_horizontal_gap" android:isModifier="true" android:isSticky="true" android:keyEdgeFlags="left" android:keyIcon="@drawable/sym_keyboard_shift" />
        <Key android:horizontalGap="@dimen/qwerty_horizontal_gap_right_of_shift" android:codes="45" android:keyLabel="-" />
        <Key android:codes="52" android:keyLabel="'" />
        <Key android:codes="52" android:keyLabel="&quot;" />
        <Key android:codes="51" android:keyLabel=":" />
        <Key android:codes="51" android:keyLabel=";" />
        <Key android:codes="44" android:keyLabel="," />
        <Key android:codes="63" android:keyLabel="\?" />
        <Key android:keyWidth="@dimen/qwerty_backspace_key_width" android:horizontalGap="@dimen/qwerty_horizontal_gap_left_of_backspace" android:codes="-5" android:keyEdgeFlags="right" android:isRepeatable="true" android:keyIcon="@drawable/qwerty_key_icon_backspace_xml" />
    </Row>

    <Row android:rowEdgeFlags="bottom" >
        <Key android:keyWidth="@dimen/qwerty_input_mode_key_width_multi_language" android:horizontalGap="@dimen/qwerty_first_col_horizontal_gap" android:codes="-2" android:keyEdgeFlags="left" android:keyLabel="\?123" />
        <Key android:codes="-3" android:keyIcon="@drawable/qwerty_key_icon_settings_xml"  />
        <Key android:codes="44" android:keyLabel="," />
        <Key android:keyWidth="@dimen/qwerty_space_key_width" android:keyIcon="@drawable/sym_keyboard_space" android:codes="32" />
        <Key android:codes="46"  android:keyLabel="." />
        <Key android:keyWidth="@dimen/qwerty_enter_key_width_multi_language" android:codes="40" android:keyEdgeFlags="right" android:keyIcon="@drawable/sym_keyboard_return" />
    </Row>

    <Row android:keyHeight="0.0px" android:verticalGap="@dimen/qwerty_last_line_vertical_gap">
        <Key android:keyWidth="100.0%p" android:codes="-257"  />
    </Row>
</Keyboard>